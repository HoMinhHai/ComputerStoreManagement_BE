<body>
    <form class="col-6" style="margin: auto; text-align: center " method="post" action="/postCRUD">
        <div class="form-group">
            <label for="email">Địa chỉ email</label>
            <input type="email" class="form-control" placeholder="Nhập vào email" name="email" id="email">

        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Mật khẩu</label>
            <input type="password" class="form-control" placeholder="Nhập vào mật khẩu" name="password">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">First Name</label>
            <input type="password" class="form-control" placeholder="Nhập vào mật khẩu" name="password">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Last Name</label>
            <input type="password" class="form-control" placeholder="Nhập vào mật khẩu" name="password">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Address</label>
            <input type="password" class="form-control" placeholder="Nhập vào mật khẩu" name="password">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Phone Number</label>
            <input type="password" class="form-control" placeholder="Nhập vào mật khẩu" name="password">
        </div>

        <button type="submit" class="btn btn-primary">Thêm mới</button>
    </form>
